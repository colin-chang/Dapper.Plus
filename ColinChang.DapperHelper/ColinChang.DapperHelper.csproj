<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Colin Chang</Authors>
    <Description>A Dapper wrapper which provides some useful helper for common data operations.</Description>
    <Copyright>https://ccstudio.org</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/colin-chang/DapperHelper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/colin-chang/DapperHelper</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>dapper,dapperplus,dapperhelper,dapper.net</PackageTags>
    <Version>1.1.1</Version>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReleaseNotes>1. remove genericity constraints
2. update dependencies.</PackageReleaseNotes>
    <PackageIconUrl>https://raw.githubusercontent.com/colin-chang/DapperHelper/master/dapper.ico</PackageIconUrl>
    <AssemblyVersion>1.1.1.0</AssemblyVersion>
    <FileVersion>1.1.1.0</FileVersion>
    <Title>DapperHelper</Title>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>default</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.35" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
